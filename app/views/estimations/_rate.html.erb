<% unless @complexity.nil?|| @estimation.deliverable.type.nil?%>
    <% unless @estimation.deliverable.historical_data(@complexity)[:productivity_rate][:max].nil?%>
        <div class="estimation-cell"><%=text_field_tag 'max_rate',  @estimation.deliverable.historical_data(@complexity)[:productivity_rate][:max],:class => 'span1' ,:disabled =>true %></div>
        <div class="estimation-cell"><%=text_field_tag 'avg_rate', @estimation.deliverable.historical_data(@complexity)[:productivity_rate][:avg],:class => 'span1' ,:disabled =>true %></div>
        <div class="estimation-cell"><%=text_field_tag 'min_rate',  @estimation.deliverable.historical_data(@complexity)[:productivity_rate][:min],:class => 'span1' ,:disabled =>true  %></div>
    <%else%>
        <div class="estimation-no-data-cell">(No Historical Data)</div>
    <%end%>
<%end%>